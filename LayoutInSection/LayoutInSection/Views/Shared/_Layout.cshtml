
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>_Layout</title>
</head>
<body>
    <div> 
        <header style="height:300px;background-color:aqua">
            <h1>is a header</h1> 
        </header>
        <div>
            @RenderBody()
        </div>
        <footer style="height:300px;background-color:aquamarine">
            <h1>is a footer</h1>
        </footer>
        <div>
            @RenderSection("credits",required:true)
        </div>
    </div>
</body>
</html>
