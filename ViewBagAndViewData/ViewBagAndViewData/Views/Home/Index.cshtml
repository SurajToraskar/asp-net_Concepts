
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>
            @ViewBag.Name
        </h1>
        <ul>
            @foreach(var item in ViewBag.myList)
            {
                <li>@item</li>
            }
        </ul>
        <ul>
            @foreach(var item in ViewBag.myEmployeeList)
            {
                <li>Id - @item.Id , Name - @item.Name , Email - @item.Email</li>
            }
        </ul>
    </div>
</body>
</html>
